{
    "manifestVersion": 1,
    "id": "semantic-tagger",
    "name": "Semantic Tagger",
    "version": "0.0.1",
    "publisher": "Matthew de Beer",
    "tags": [
        "<<ARRAY OF MARKETPLACE TAGS>>"
    ],
    "galleryFlags": [
        "Public"
    ],
    "content": {
        "license": {
          "path": "LICENSE"
        },
        "details": {
          "path": "README.md"
        }
    },
    "links": {
        "support": {
          "uri": "https://github.com/setagana/semantic-tagger/issues"
        },
        "repository": {
          "uri": "https://github.com/setagana/semantic-tagger"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/setagana/semantic-tagger"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "A build pipeline task to add a timestamp pre-release tag to a semantic version.",
    "categories": [
        "Azure Pipelines"
    ],
    "screenshots": [{
        "path": "<<EXTENSION SCREENSHOT>>"
      }
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "<<FOLDER NAME IN THIS REPO THAT CONTAINS THE EXTENSION SRC>>"
        }
    ],
    "contributions": [
        {
            "id": "version-extractor-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "semantic-tagger-task"
            }
        }
    ]
}