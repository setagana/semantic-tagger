{
    "manifestVersion": 1,
    "id": "semantic-tagger",
    "name": "Semantic Tagger",
    "version": "1.0.0",
    "publisher": "MatthewdeBeer",
    "tags": [
        "build",
        "semantic version",
        "semver",
        "versioning",
        "version",
        "Utility task"
    ],
    "content": {
        "license": {
          "path": "LICENSE"
        },
        "details": {
          "path": "ReadMe.md"
        }
    },
    "galleryFlags": [ 
        "Public" 
    ],
    "links": {
        "support": {
          "uri": "https://github.com/setagana/semantic-tagger/issues"
        },
        "repository": {
          "uri": "https://github.com/setagana/semantic-tagger"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/setagana/semantic-tagger"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "A build pipeline task to add a timestamp pre-release tag to a semantic version.",
    "categories": [
        "Azure Pipelines"
    ],
    "screenshots": [{
        "path": "images/SemanticTagger.png"
      }
    ],
    "icons": {
        "default": "images/extension-icon.png"        
    },
    "files": [
        {
            "path": "semantic-tagger-task"
        }
    ],
    "contributions": [
        {
            "id": "version-extractor-build-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "semantic-tagger-task"
            }
        }
    ]
}